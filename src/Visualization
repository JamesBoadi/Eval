export default function Box(props: Props) {
  const renderBox = () => {
    return (
      <rect
        onMouseEnter={(e) => {
          console.log(`${props.className}`);
        }}
        width="400" // can also adjust to loading bar animation
        height="100"
        style={{ fill:`${props.color}`}}
       // style={{ backgroundColor:`${props.color}`}}
      />
    );
  };

  return (
    <>
      <svg>
        {renderBox()}
      </svg>
    </>
  );
}
